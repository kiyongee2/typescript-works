{
  // Visit https://aka.ms/tsconfig to read more about this file
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES6",
    "outDir": "./dist",
    "moduleResolution": "node",
    "strict": true,
    "baseUrl": ".",
    "sourceMap": true,
    "downlevelIteration": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "paths": {"*": ["node_modules/*"]}
  },
  "include": ["src/**/*"],
}
